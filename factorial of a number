#include<stdio.h>
long factorial(int);
int main()
{
int n;
long f;
printf("enter a number");]
scanf("%d",&n);
if(n<0)
{
printf("factorial is not defined");
}
else{
f=factorial(n);
printf("factorial is %d",f);
}
return 0;
}
long factorial(int n)
{
if (n==0)
return 1;
else
return(n*factorial(n-1));
}
}
